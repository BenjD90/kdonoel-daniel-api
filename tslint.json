{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		"no-var-requires": false,
		"curly": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"no-any": false,
		"no-null-keyword": false,
		"cyclomatic-complexity": [
			true,
			20
		],
		"trailing-comma": [
			false,
			{
				"multiline": "always",
				"singleline": "never"
			}
		],
		"interface-name": [
			true,
			"never-prefix"
		],
		"no-console": [
			true,
			"log",
			"debug",
			"info",
			"time",
			"timeEnd",
			"trace"
		],
		"array-type": [
			true,
			"array"
		],
		"class-name": true,
		"prefer-const": true,
		"semicolon": [
			true,
			"always"
		],
		"max-line-length": [
			true,
			200
		],
		"no-string-literal": false,
		"no-use-before-declare": true,
		"no-conditional-assignment": true,
		"label-position": true,
		"no-shadowed-variable": true,
		"triple-equals": [
			true,
			"allow-undefined-check",
			"allow-null-check"
		],
		"quotemark": [
			true
		],
		"variable-name": [
			true,
			"allow-leading-underscore",
			"allow-pascal-case",
			"ban-keywords",
			"check-format"
		],
		"indent": [
			true,
			"tabs"
		]
	}
}
